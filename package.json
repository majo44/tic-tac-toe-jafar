{
    "name": "tic-tac-toe-jafar",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run compile:watch\" \"npm run run:watch\"",
        "compile": "tsc",
        "compile:watch": "tsc -w",
        "run": "node dist/server/index.js",
        "run:watch": "node --watch dist/server/index.js",
        "test": "node --import tsx --watch --test ./src/server/checkGameResult.test.ts"
    },
    "dependencies": {
        "concurrently": "^8.2.2",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.18.0",
        "typescript": "^5.2.2",
        "tsx": "4.7.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.6",
        "@types/express": "4.17.20",
        "@types/node": "^20.8.9",
        "@types/react": "^18.2.33",
        "@types/react-dom": "^18.2.14"
    }

}
